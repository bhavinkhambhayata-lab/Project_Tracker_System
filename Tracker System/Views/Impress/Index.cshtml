<script type="text/javascript">
    var editImpressDetailRowId = '';
    function GetData(tab) {
        if (tab == "tabDetails") {

            if (editImpressDetailRowId != '') {

                $.ajax({
                    //url: '/Impress/GetImpressEditDetailsTab',
                    url: '@Url.Action("GetImpressEditDetailsTab","Impress")',
                    type: 'GET',
                    data: { rowID: editImpressDetailRowId },
                    success: function (data) {

                        $('#tabDetails').html(data);
                    },
                    error: function () {
                        alert('Failed to load detail');
                    }
                });
            } else {

                $.ajax({
                    //url: '/Impress/GetImpressAddDetailsTab',
                    url: '@Url.Action("GetImpressAddDetailsTab","Impress")',
                    type: 'GET',
                    success: function (result) {

                        $('#tabDetails').html(result);
                    }
                });
            }
        }
        else if (tab == "tabOtherReport") {
            editImpressDetailRowId = '';
            debugger
            $.ajax({
                //url: '/Impress/GetImpressOtherReportTab',
                url: '@Url.Action("GetImpressOtherReportTab","Impress")',
                type: 'GET',
                success: function (result) {
                    $('#tabOtherRpt').html(result);
                }
            });
        }
        else if (tab == "tabCommecialTab") {
            editImpressDetailRowId = '';

            $.ajax({
                //url: '/Impress/GetImpressPendingCommercialTabList?GroupId=' + 2,
                url: '@Url.Action("GetImpressPendingCommercialTabList","Impress", new { GroupId = 2 })',
                type: 'GET',
                success: function (result) {
                    $('#tabCommercial').html(result);
                }
            });
        }
        else if (tab == "tabHODApproval") {
            editImpressDetailRowId = '';

            $.ajax({
                //url: '/Impress/GetImpressPendingHODApprovalTabList?GroupId=' + 3,
                url: '@Url.Action("GetImpressPendingHODApprovalTabList","Impress", new { GroupId = 3 })',
                type: 'GET',
                success: function (result) {
                    $('#tabHODApproval').html(result);
                }
            });
        }
        else if (tab == "tabMKTHead") {
            editImpressDetailRowId = '';

            $.ajax({
                //url: '/Impress/GetImpressPendingMKTHeadTabList?GroupId=' + 4,
                url: '@Url.Action("GetImpressPendingMKTHeadTabList","Impress", new { GroupId = 4 })',
                type: 'GET',
                success: function (result) {
                    $('#tabMktHead').html(result);
                }
            });
        }
        else if (tab == "tabDirectors") {
            editImpressDetailRowId = '';

            $.ajax({
                //url: '/Impress/GetImpressPendingDirectorsApprovalTabList?GroupId=' + 5,
                url: '@Url.Action("GetImpressPendingDirectorsApprovalTabList","Impress", new { GroupId = 5 })',
                type: 'GET',
                success: function (result) {
                    $('#tabDirectors').html(result);
                }
            });
        }
        else if (tab == "tabSubmmitedAccount") {
            editImpressDetailRowId = '';

            $.ajax({
                //url: '/Impress/GetImpressPendingSubmittedAccountTabList?GroupId=' + 6,
                url: '@Url.Action("GetImpressPendingSubmittedAccountTabList","Impress", new { GroupId = 6 })',
                type: 'GET',
                success: function (result) {
                    $('#tabSubmitted-Account').html(result);
                }
            });
        }
        //else if (tab == "tabConfimationRecieved") {
        //    editImpressDetailRowId = '';

        //    //$.ajax({
        //    //    url: '/TSD/GetTSDPendingCourieredTabList?GroupId=' + 8,
        //    //    type: 'GET',
        //    //    success: function (result) {
        //    //        $('#tabCourieredOn').html(result);
        //    //    }
        //    //});
        //}
        else {
            editImpressDetailRowId = '';

            $.ajax({
                //url: '/Impress/ImpressListTab',
                url: '@Url.Action("ImpressListTab","Impress")',
                type: 'GET',
                success: function (result) {
                    $('#tabList').html(result);
                },
                error: function () {
                    alert('error');
                }
            });
        }
    }
    $(document).ready(function () {
        GetData('tabList');
    });
</script>

