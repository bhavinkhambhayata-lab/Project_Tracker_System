

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm())
{
    <section>
        <div class="box" style="width:auto;">
            <div class="box-body" style="width:auto;">
                <div class="clearfix"></div>
                <div id="tabs" class="nav-tabs-custom" style=" margin-top: -23px !important;">
                    <hr>
                    <ul class="nav nav-tabs" style=" margin-top: -6px !important; font-size:15px;">
                        <li class="HeaderFont"><a id="lnkDetails" href="#tabDetails" style="font-size:15px!important;" data-toggle="tab" onclick="GetData('tabDetails');"><b>@resourceSettings.getResourceLable("lblDetails", "common", "Details")</b></a></li>
                        <li class="HeaderFont active"><a id="lnkList" style="font-size:15px!important;" href="#tabList" data-toggle="tab" onclick="GetData('tabList');"><b>@resourceSettings.getResourceLable("lblList", "common", "List")</b></a></li>
                        <li><a id="lnkOtherRpt" class="HeaderFont" style="font-size:15px!important;" href="#tabOtherRpt" data-toggle="tab" onclick="GetData('tabOtherReport');"><b>@resourceSettings.getResourceLable("lblOtherRpt", "common", "OtherRpt")</b></a></li>
                        <li><a id="lnkCommercial" class="HeaderFont" style="font-size:15px!important;" href="#tabCommercial" data-toggle="tab" onclick="GetData('tabCommecialTab');"><b>@resourceSettings.getResourceLable("lblCommercial", "common", "Commercial")</b></a></li>
                        <li><a id="lnkHODApproval" class="HeaderFont" style="font-size:15px!important;" href="#tabHODApproval" data-toggle="tab" onclick="GetData('tabHODApproval');"><b>@resourceSettings.getResourceLable("lblHODApproval", "common", "HOD Approval")</b></a></li>
                        <li><a id="lnkMktHead" class="HeaderFont" style="font-size:15px!important;" href="#tabMktHead" data-toggle="tab" onclick="GetData('tabMKTHead');"><b>@resourceSettings.getResourceLable("lblMktHead", "common", "Mkt Head")</b></a></li>
                        <li><a id="lnkDirectors" class="HeaderFont" style="font-size:15px!important;" href="#tabDirectors" data-toggle="tab" onclick="GetData('tabDirectors');"><b>@resourceSettings.getResourceLable("lblDirector'sApproval", "common", "CMO's Approval")</b></a></li>
                        <li><a id="lnkSubmitted_Account" class="HeaderFont" style="font-size:15px!important;" href="#tabSubmitted-Account" data-toggle="tab" onclick="GetData('tabSubmmitedAccount');"><b>@resourceSettings.getResourceLable("lblSubmitted-Account", "common", "Submitted-Account")</b></a></li>
                        @*<li><a id="lnkConfimationRecieved" class="HeaderFont" style="font-size:15px!important;" href="#tabConfimationRecieved" data-toggle="tab" onclick="GetData('tabConfimationRecieved');"><b>@resourceSettings.getResourceLable("lblConfimationRecieved", "common", "Confimation Recieved")</b></a></li>*@
                    </ul>
                </div>
                <div class="tab-content">
                    <div id="tabDetails" class="tab-pane">
                    </div>

                    <div id="tabList" class="tab-pane active">
                    </div>

                    <div id="tabOtherRpt" class="tab-pane">
                    </div>

                    <div id="tabCommercial" class="tab-pane">
                    </div>

                    <div id="tabHODApproval" class="tab-pane">
                    </div>

                    <div id="tabMktHead" class="tab-pane">
                    </div>

                    <div id="tabDirectors" class="tab-pane">
                    </div>

                    <div id="tabSubmitted-Account" class="tab-pane">
                    </div>

                    @*<div id="tabConfimationRecieved" class="tab-pane">
                        </div>*@

                </div>

            </div>
        </div>
    </section>
}

